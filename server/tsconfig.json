{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "downlevelIteration": true,
    "module": "commonjs",
    "paths": {
      "src/*": ["src/*"],
      "routes/*": ["src/routes/*"]
    },
    "sourceMap": false,
    "strict": true,
    "target": "es2019",
    "outDir": "./dist"
  },
  "exclude": ["node_modules"],
  "include": ["./src/*", "index.ts"]
}
